{
  "name": "codename.co",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:kilianc/codename.co.git"
  },
  "scripts": {
    "install-hooks": "ln -sf ../../hooks/pre-push .git/hooks/pre-push && chmod +x .git/hooks/*",
    "deploy-dev": "echo \"require('./scripts/deploy').deploy('refs/heads/develop')\" | node",
    "deploy-stag": "echo \"require('./scripts/deploy').deploy('refs/heads/staging')\" | node",
    "deploy-prod": "echo \"require('./scripts/deploy').deploy('refs/heads/master')\" | node"
  },
  "dependencies": {
    "async": "^0.9.0",
    "cli-color": "^0.3.2",
    "config": "^0.4.36"
  },
  "devDependencies": {
    "bower": "^1.3.5",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-concat": "^0.4.0",
    "grunt-contrib-connect": "^0.8.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-cssmin": "^0.10.0",
    "grunt-contrib-htmlmin": "^0.3.0",
    "grunt-contrib-sass": "^0.7.3",
    "grunt-contrib-uglify": "^0.5.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-ngmin": "0.0.3",
    "grunt-usemin": "^2.2.0",
    "matchdep": "^0.3.0"
  }
}
